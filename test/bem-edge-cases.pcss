.cells-Introspector-
	table:
		padding: 0px

.button-
	.-label:
		padding: 0px

		
.checkbox-:
	user-select: none
	.-icon:
		display: inline-block
		&:before, &:after:
			content: ""


.ProjectStructure-
	min-width: 318dp
	.-footer
		display: none
		height: auto
		.-button:
			span:
				pointer-events: none
			&:nth-child(2)
				margin-right: 1.3em

.cells-Introspector-
	.-cells-:
		table:
			tr:
				td, th:
					padding: 0px
					font-size: 10px
					border: 0px solid transparent
